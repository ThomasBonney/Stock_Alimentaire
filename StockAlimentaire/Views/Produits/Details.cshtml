@model StockAlimentaire.Models.Produit

@{
    ViewData["Title"] = "Details";
}

<h2>Produit - Details</h2>

<div>
    <h4>Produit</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt class="edit-label">
            Nom
        </dt>
        <dd>
            @Html.DisplayFor(model => model.nomProd)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.idProd">Modifier</a> |
    <a asp-action="Index">Retour à la liste des produits</a>
</div>
